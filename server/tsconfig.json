{
    "compilerOptions": {
        /* Visit https://aka.ms/tsconfig to read more about this file */
        /*Language and Environment*/
        "target": "es5",

        /* Modules */
        "module": "commonjs",
        //"rootDirs": [],

        /* Interop Constraints */
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,

        /* Type Checking */
        "strict": true,

        /* Completeness */
        "skipLibCheck": true

        /* Emit */
        //"outFile": "./",
        //"outDir": "./",
    }
}
